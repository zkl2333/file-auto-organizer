{
  "name": "file-auto-organizer",
  "version": "1.0.0",
  "description": "File Auto Organizer",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm",
    "start": "bun run dist/index.js",
    "dev": "bun run src/index.ts",
    "dry": "bun run dist/index.js --dry-run --once",
    "once": "bun run dist/index.js --once",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkl2333/file-auto-organizer.git"
  },
  "author": "zkl2333",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkl2333/file-auto-organizer/issues"
  },
  "homepage": "https://github.com/zkl2333/file-auto-organizer#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@types/js-yaml": "^4.0.9",
    "exiftool-vendored": "^30.4.0",
    "fast-levenshtein": "^3.0.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "openai": "^5.16.0",
    "pino": "^9.3.2"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "^0.0.4",
    "@types/node": "^20.14.11",
    "@types/node-cron": "^3.0.11",
    "bun-types": "latest",
    "typescript": "^5.6.2"
  },
  "trustedDependencies": [
    "exiftool-vendored"
  ],
  "engines": {
    "bun": ">=1.0.0"
  }
}
